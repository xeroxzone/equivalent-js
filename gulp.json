{
  "vendors" : {
    "src" : [
      "node_modules/jquery/dist/jquery.min.js",
      "node_modules/material-components-web/dist/material-components-web.js",
      "node_modules/material-components-web/dist/material-components-web.min.js"
    ],
    "dest" : "web/js/lib"
  },
  "docs" : {
    "src" : [
      "src/lib/equivalent/**/*.js",
      "node_modules/equivalent-js-*/src/lib/**/*.js",
      "node_modules/equivalent-js-*/src/lib/*.js",
      "docs/**/*.md",
      "docs/*.md",
      "*.md"
    ],
    "dest" : "web/doc"
  },
  "testunit" : {
    "src" : [
      "node_modules/qunitjs/qunit/qunit.js"
    ],
    "dest" : "web/js/lib"
  },
  "tests" : {
    "src" : [
      "src/test/**/*.js"
    ],
    "dest" : "web/js"
  },
  "configs" : {
    "src" : [
      "src/config/*.json"
    ],
    "dest" : "web/js/config"
  },
  "plugins" : {
    "config": "node_modules/equivalent-js-plugin-*/plugin.json",
    "classes": {
      "src": "node_modules/equivalent-js-plugin-*/src/lib/**.js",
      "dest": "web/js/lib/equivalent/Plugin"
    },
    "tests": {
      "src" : "node_modules/equivalent-js-plugin-*/src/test/lib/**.js",
      "dest" : "web/js/test/lib/equivalent/Plugin"
    },
    "layout": {
      "src": "node_modules/equivalent-js-plugin-*/src/lib/**.scss",
      "dest": "web/css/Plugin"
    }
  },
  "scripts" : {
    "src" : [
      "src/lib/**/*.js",
      "!src/lib/ui"
    ],
    "dest" : "web/js"
  },
  "apps" : {
    "src" : [
      "src/app/**/*.js"
    ],
    "dest" : "web/js"
  },
  "styles" : {
    "src" : [
      "src/app/**/*.scss",
      "src/lib/ui/**",
      "node_modules/material-components-web/dist/material-components-web.css",
      "node_modules/material-components-web/dist/material-components-web.min.css"
    ],
    "dest" : "web/css"
  },
  "minify": {
    "name": "equivalent.min.js",
    "src": [
      "src/lib/equivalent/System.js",
      "src/lib/equivalent/Plugin.js",
      "src/lib/equivalent/Manager.js",
      "src/lib/equivalent/Manager/Extend.js",
      "src/lib/equivalent/Manager/Controller.js",
      "src/lib/equivalent/Manager/App.js",
      "src/lib/equivalent/Renderer.js"
    ],
    "dest": "web/js/lib"
  }
}
