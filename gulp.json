{
  "vendors" : {
    "src" : [
      "node_modules/jquery/dist/jquery.min.js",
      "node_modules/material-components-web/dist/material-components-web.js"
    ],
    "dest" : "web/js/lib"
  },
  "docs" : {
    "src" : [
      "src/lib/equivalent/**/*.js",
      "doc/**/*.md",
      "doc/*.md",
      "*.md"
    ],
    "dest" : "web/doc"
  },
  "testunit" : {
    "src" : [
      "node_modules/qunitjs/qunit/qunit.js"
    ],
    "dest" : "web/js/lib"
  },
  "tests" : {
    "src" : [
      "src/test/**/*.js"
    ],
    "dest" : "web/js"
  },
  "configs" : {
    "src" : [
      "src/config/*.json"
    ],
    "dest" : "web/js/config"
  },
  "apps" : {
    "src" : [
      "src/app/**/*.js"
    ],
    "dest" : "web/js"
  },
  "scripts" : {
    "src" : [
      "src/lib/**/*.js",
      "!src/lib/ui"
    ],
    "dest" : "web/js"
  },
  "styles" : {
    "src" : [
      "src/app/**/*.scss",
      "src/lib/ui/**",
      "node_modules/material-components-web/dist/material-components-web.css"
    ],
    "dest" : "web/css"
  },
  "minify": {
    "name": "equivalent.min.js",
    "src": [
      "src/lib/equivalent/System.js",
      "src/lib/equivalent/Manager.js",
      "src/lib/equivalent/Manager/Extend.js",
      "src/lib/equivalent/Manager/Controller.js",
      "src/lib/equivalent/Manager/App.js",
      "src/lib/equivalent/Renderer.js"
    ],
    "dest": "web/js/lib"
  }
}
